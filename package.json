{
  "name": "ioo",
  "version": "0.1.5",
  "description": "Immutable Object Operators",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "prebuild": "yarn",
    "build": "tsc",
    "test": "npm run lint && jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "prerelease": "npm run build && npm test",
    "release": "standard-version && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/this-dot/ioo.git"
  },
  "keywords": [
    "typescript",
    "library",
    "immutability",
    "utilities"
  ],
  "author": "Hagai Cohen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/this-dot/ioo/issues"
  },
  "homepage": "https://github.com/this-dot/ioo#readme",
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "@types/node": "^8.0.22",
    "@types/ramda": "^0.24.7",
    "eslint": "^4.4.1",
    "eslint-plugin-typescript": "^0.5.0",
    "jest": "^20.0.4",
    "standard-version": "^2.4.0",
    "ts-jest": "^20.0.10",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "2.4.0",
    "typescript-eslint-parser": "^5.0.1"
  },
  "peerDependencies": {
    "tslint": "^5.6.0"
  },
  "dependencies": {
    "ramda": "0.24.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
